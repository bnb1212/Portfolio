package pack.auto;

import org.springframework.beans.factory.annotation.Autowired;

public class SenderProcess {
	@Autowired
	private Sender sender;
	
	public void setSender(Sender sender) {
		this.sender = sender;
		
	}
	
	public Sender getSender() {
		return sender;
	}
	public void dispData() {
		sender.show();
	}
}
