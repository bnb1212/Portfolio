package pack;

public class MessageImpl implements MessageInter {
	private String name1, name2;
	private int year;

	public MessageImpl(String name1, String name2, int year) { // Constuctor Injection 하는중
		this.name1 = name1;
		this.name2 = name2;
		this.year = year;

	}

	public void sayHi() {
		// 출력 담당
		String msg = name1 + " " + name2;
		msg += "\n" + (year + 20);
		System.out.println(msg);

	}

}
