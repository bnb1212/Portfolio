package pack.equip;

public class EquipDto {
	private int equip_no;
	private String equip_name, equip_stock, equip_info, equip_image;
	
	public int getEquip_no() { 
		return equip_no;
	}
	public void setEquip_no(int equip_no) {
		this.equip_no = equip_no;
	}
	public String getEquip_name() {
		return equip_name;
	}
	public void setEquip_name(String equip_name) {
		this.equip_name = equip_name;
	}
	public String getEquip_stock() {
		return equip_stock;
	}
	public void setEquip_stock(String equip_stock) {
		this.equip_stock = equip_stock;
	}
	public String getEquip_info() {
		return equip_info;
	}
	public void setEquip_info(String equip_info) {
		this.equip_info = equip_info;
	}
	public String getEquip_image() {
		return equip_image;
	}
	public void setEquip_image(String equip_image) {
		this.equip_image = equip_image;
	}
	
	
}
